{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": ["**/*.test.js", "**/*.spec.js"],
    "rules": {
        "max-len": [
            "error",
            {
                "ignoreStrings": true
            }
        ]
    }
}